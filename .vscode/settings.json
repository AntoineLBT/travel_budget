{
    "editor.formatOnSave": true,
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        }
    },
    "python.analysis.typeCheckingMode": "off",
    "python.analysis.autoImportCompletions": false,
    "flake8.args": [
        "--max-line-length=88"
    ],
    "html.format.enable": false
}